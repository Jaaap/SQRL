{
	"name": "__MSG_name__",
	"description": "__MSG_description__",
	"default_locale": "en",
	"manifest_version": 2,
	"version": "0.0.1",
	"homepage_url": "https://github.com/Jaaap/SQRL",
	"applications": {
		"gecko": {
			"id": "sqrl@pass.dog"
		}
	},
	"icons": {
		"256": "icons/256.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"],
			"run_at": "document_end"
		}
	],
	"permissions": [
	],
	"browser_action": {
		"browser_style": true,
		"default_icon": "icons/256.png",
		"default_title": "SQRL",
		"default_popup": "__MSG_popup__"
	},
	"background": {
		"scripts": ["BigInteger.js","sodium-asmjs.js","background.js"],
		"persistent": false
	}
}
